<div class="row">
    <div class="col-lg-12">
        <div class="card m-b-20" style="max-width: 100%;">
            <div class="card-body">
                <div class="row" style="padding-bottom:15px;">
                    <div class="col-sm-4">
                        <div class="input-group">
                        <label class="col-form-label">From &nbsp;</label>
                            <input type="text" class="form-control" placeholder="mm/dd/yyyy" id="datepicker-autoclose">
                            <div class="input-group-append">
                                <span class="input-group-text"><i class="mdi mdi-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="input-group">
                            <label class="col-form-label">To &nbsp;</label>
                            <input type="text" class="form-control" placeholder="mm/dd/yyyy" id="to-datepicker-autoclose">
                            <div class="input-group-append">
                                <span class="input-group-text"><i class="mdi mdi-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <input class="form-control" required="" placeholder="Search Campaign" id="search_camp" type="text">
                    </div>
                </div>
                <table id="datatable" class="table table-bordered">
                    <thead>
                    <tr>
                        <th>Campaign Name</th>
                        <th>Sender ID</th>
                        <th>Message</th>
                        <th>Contact File</th>
                        <th>Media Name</th>
                        <th>SMS Used</th>
                        <th>Sent Date</th>
                        <th>Scheduled Time</th>
                        <th>View Clicks</th>
                        <th>Detailed Report</th>
                        <th>Bulk SMS/Quick SMS</th>
                        <th>Track Clicks</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <th>XYZ</th>
                        <th>#f767as</th>
                        <th>Test Sms</th>
                        <th>txtMobil.xls</th>
                        <th>ABCD Media</th>
                        <th>897</th>
                        <th>12th Oct 2018</th>
                        <th>10:00 AM</th>
                        <th><a href="#">10</a></th>
                        <th>SMS Sent for Campaign</th>
                        <th>Bulk SMS</th>
                        <th>abc</th>
                    </tr>
                    </tbody>
                </table>
                <div class="form-group row">
                    <div class="col-sm-12">
                        <button type="submit" class="btn btn-success waves-effect waves-light">
                            Delete
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- end col -->
</div> <!-- end row -->

<script type="text/javascript">
    $(document).ready(function() {
        $(".card").attr("id","camp_dashboard");
        $('form').parsley();
        $("#page_title").html(<%- JSON.stringify(title) %>);
    });
</script>

