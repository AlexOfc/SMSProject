<div class="row">
    <div class="col-lg-12">
        <div class="card m-b-20">
            <div class="card-body">

                <form class="" action="#">
                    <div class="form-group row">
                    <div class="col-sm-12">
                        <ul class="ul-tick">
                            <li>
                                <i class="tick-mark ti-check-box"></i>
                                <span class="text-muted m-b-30 font-14">
                                    Please select the groups the contact should belong to
                                </span>
                            </li>
                            <li>
                                <i class="tick-mark ti-check-box"></i>
                                <span class="text-muted m-b-30 font-14">
                                3 - 50 characters e.g. Aman Verma, amit
                                </span>
                            </li>
                            <li>
                                <i class="tick-mark ti-check-box"></i>
                                <span class="text-muted m-b-30 font-14">
                                    Add country code as prefix without any spaces e.g. 919898989898
                                </span>
                            </li>
                            <li>
                            <i class="tick-mark ti-check-box"></i>
                            <span class="text-muted m-b-30 font-14">
                            Max 50 characters e.g.username@company.com
                            </span>
                            </li>
                        </ul>
                    </div>
                    </div>
                    <div class="form-group row">
                        <label for="pb_group_name" class="col-sm-3 col-form-label"> Group </label>
                        <div class="col-sm-9">
                           <select class="form-control select2">
                                <option value="1">Group1</option>
                                <option value="2">Group2</option>
                            </select>

                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Name</label>
                        <div class="col-sm-9">
                        <input type="text" class="form-control" required data-parsley-minlength="3" maxlength="30"  placeholder="Group Name"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Mobile Number</label>
                        <div class="col-sm-9">
                        <input data-parsley-type="digits" type="text" class="form-control" required  data-parsley-minlength="10"  maxlength="10" placeholder="Mobile Number"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Email Id</label>
                        <div class="col-sm-9">
                            <input type="email" class="form-control" required parsley-type="email" placeholder="Enter a valid e-mail"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label">Designation</label>
                        <div class="col-sm-9">
                        <input type="text" class="form-control" required data-parsley-minlength="3" maxlength="30"  placeholder="Designation"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-3 col-form-label"></label>
                        <div class="col-sm-9">
                            <button type="submit" class="btn btn-success waves-effect waves-light">
                                Add
                            </button>
                            <button type="reset" class="btn btn-secondary waves-effect m-l-5">
                                Reset
                            </button>
                        </div>
                    </div>
                </form>

            </div>
        </div>
    </div> <!-- end col -->
</div> <!-- end row -->

<script type="text/javascript">
    $(document).ready(function() {
        //$('form').parsley();
        $("#page_title").html(<%- JSON.stringify(title) %>);

    });
</script>

<script src="plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
<script src="plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
<script src="plugins/select2/js/select2.min.js" type="text/javascript"></script>
<script src="plugins/bootstrap-maxlength/bootstrap-maxlength.min.js" type="text/javascript"></script>
<script src="plugins/bootstrap-filestyle/js/bootstrap-filestyle.min.js" type="text/javascript"></script>
<script src="plugins/bootstrap-touchspin/js/jquery.bootstrap-touchspin.min.js" type="text/javascript"></script>

<!-- Plugins Init js -->
<script src="pages/form-advanced.js"></script